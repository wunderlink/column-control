{
  "name": "column-control",
  "version": "0.1.4",
  "description": "decorate an html table with basic column controls",
  "main": "index.js",
  "scripts": {
    "example": "beefy example/basic.js --live",
    "test": "testling"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wunderlink/column-control.git"
  },
  "devDependencies": {
    "browserify": "~2.35.1",
    "coffee-script": "~1.6.3",
    "beefy": "~0.4.2",
    "tape": "^2.12.1",
    "testling": "^1.6.1"
  },
  "dependencies": {
    "coffeeify": "~0.5.2",
    "cssify": "~0.4.1"
  },
  "keywords": [
    "table",
    "columns"
  ],
  "author": "Tyler Bushnell",
  "license": "BSD",
  "browser": "./lib/index.coffee",
  "browserify": {
    "transform": [
